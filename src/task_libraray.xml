		<entry>
			<key>201</key>
			<value id="201" taskName="NSDC_FORM">
				<description>This form need to be filled by students</description>
				<label>Certificate Requisition Form</label>
				<steps label="Personal Profile" id="1">
					<form_elements id="1" required="true">
						<dataType>number</dataType>
						<label>Pincode</label>
						<elemntName>pincode</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select distinct pin as key, pin as value from pincode where CAST(pin AS varchar) like '%:search_term%'
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="2" required ="true">
						<dataType> date</dataType>
						<label>Date of Birth</label>
						<elemntName>dob</elemntName>
						<elemntType>DATE_PICKER</elemntType>
					</form_elements>
					<form_elements id="3" required ="true">
						<dataType>number</dataType>
						<label>Aadhar No</label>
						<elemntName>aadhar</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="4">
						<dataType>string</dataType>
						<label>Address Line 1 *</label>
						<elemntName>address_line_1</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="5">
						<dataType>string</dataType>
						<label>Address Line 2 *</label>
						<elemntName>address_line_2</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="6">
						<dataType>string</dataType>
						<label>Fathers Name *</label>
						<elemntName>father_name</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="7">
						<dataType>string</dataType>
						<label>Place of Birth *</label>
						<elemntName>place_of_birth</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="8">
						<dataType>boolean</dataType>
						<label>Below Poverty Line *</label>
						<elemntName>below_poverty_line</elemntName>
						<elemntType>SWITCH</elemntType>
					</form_elements>
					<form_elements id="9">
						<dataType>string</dataType>
						<label>Caste Category *</label>
						<elemntName>caste_category</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="static_list">
						<itemSource>Gen!#OBC!#SC!#ST!#PH
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="10">
						<dataType>string</dataType>
						<label>Religion *</label>
						<elemntName>religion</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="static_list"><itemSource>Hindu!#Muslim!#Christian!#Sikh!#Buddhist!#Jews!#Other</itemSource></dropdownData>
					</form_elements>
					<updateQuery>update address set pincode_id =(select id from pincode where pin = ':pincode' limit 1),addressline1=':address_line_1',addressline2=':address_line_2'  where id = (select address_id from user_profile where user_id =:USER_ID);
					 update user_profile set dob =':dob' , aadhar_no = :aadhar , caste_category =':caste_category',religion =':religion',place_of_birth=':place_of_birth',father_name=':father_name' where user_id=:USER_ID;
					 update professional_profile set below_poverty_line = ':below_poverty_line' where user_id = :USER_ID;
					</updateQuery>
				</steps>
				<steps label="Education Details" id="2">
					<form_elements id="1">
						<dataType>number</dataType>
						<label>10th Marks *</label>
						<elemntName>marks_10</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="2">
						<dataType>number</dataType>
						<label>10th Year of Passing *</label>
						<elemntName>yop10</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="3">
						<dataType>number</dataType>
						<label>12th Marks *</label>
						<elemntName>marks_12</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="4">
						<dataType>number</dataType>
						<label>12th Year of Passing *</label>
						<elemntName>yop12</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="5">
						<dataType>string</dataType>
						<label>UG Degrees</label>
						<elemntName>ug_degree</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select distinct parent_value as key, parent_value as value  from parent_dropdown_table where group_name='ug_degrees' and lower(parent_value) like '%:search_term%'
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="6" dependency="5">
						<dataType>string</dataType>
						<label>UG Specialization</label>
						<elemntName>ug_specialization</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select child_dropdown_value as key, child_dropdown_value as value from child_dropdown_table where parent_value=':dependency_term' and lower(child_dropdown_value) like '%:search_term%' ;
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="7">
						<dataType>number</dataType>
						<label>UG Marks</label>
						<elemntName>ug_marks</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="8">
						<dataType>string</dataType>
						<label>PG Degrees</label>
						<elemntName>pg_degree</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select distinct parent_value as key, parent_value as value  from parent_dropdown_table where group_name='pg_degrees' and lower(parent_value) like '%:search_term%'
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="9" dependency="8">
						<dataType>string</dataType>
						<label>PG Specialization</label>
						<elemntName>pg_specialization</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select child_dropdown_value as key, child_dropdown_value as value from child_dropdown_table where parent_value=':dependency_term' and lower(child_dropdown_value) like '%:search_term%' ;
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="10">
						<dataType>number</dataType>
						<label>PG Marks</label>
						<elemntName>pg_marks</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<updateQuery>
					 update professional_profile set marks_10=:marks_10,marks_12=:marks_12,yop_10=:yop10,yop_12=:yop12,under_graduation_specialization_name=':ug_specialization',under_gradution_marks=:ug_marks,post_graduation_specialization_name=':pg_specialization',post_gradution_marks=:pg_marks,under_graduate_degree_name=':ug_degree',pg_degree_name=':pg_degree' where user_id=:USER_ID;
					</updateQuery>
				</steps>
				<steps label="Job Details" id="3">
					<form_elements id="1">
						<dataType>string</dataType>
						<label>Job Sector *</label>
						<elemntName>job_sector</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="static_list">
						<itemSource>IT!#Automotive!#Banking and Financial!#ITES!#Healthcare!#Capital Goods!#Construction!#Security!#Telecom!#Retail!#Agriculture!#Electronics!#Beauty and Wellness!#Gem and Jewellery!#Media and Entertainment
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="2">
						<dataType>string</dataType>
						<label>Preferred Location *</label>
						<elemntName>preferred_location</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="static_list">
						<itemSource>Anywhere!#Mumbai!#Delhi!#Bangalore!#Pune!#Kolkata!#Hyderabad!#Bhopal!#Ahmedabad!#Nagpur!#Chennai!#Chandigarh
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements>
						<dataType>string</dataType>
						<label>Company Name</label>
						<elemntName>company_name</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements>
						<dataType>number</dataType>
						<label>Duration</label>
						<elemntName>duration</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements>
						<dataType>string</dataType>
						<label>Postition</label>
						<elemntName>position</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements>
						<dataType>string</dataType>
						<label>Description</label>
						<elemntName>description</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<updateQuery>
					update professional_profile set job_sector=':job_sector',preferred_location=':preferred_location',company_name=':company_name',position=':position',duration=:duration,description=':description'  where user_id=:USER_ID;
					</updateQuery>
				</steps>
				<steps label="Further Studies Details" id="3">
				<form_elements id="1">
					<dataType>boolean</dataType>
					<label>Is Studying Further After Degree? *</label>
					<elemntName>is_studying_further_after_degree</elemntName>
					<elemntType>SWITCH</elemntType>
				</form_elements>
				<form_elements id="2">
						<dataType>string</dataType>
						<label>Interested In Type of Course *</label>
						<elemntName>interested_in_type_of_course</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="static_list">
						<itemSource>PG!#Diploma!#ShortTermCourses
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="3">
						<dataType>string</dataType>
						<label>Area of Interest *</label>
						<elemntName>area_of_interest</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="static_list">
						<itemSource>Finance!#Software!#Accounting!#Marketing!#Analytics
						</itemSource>
						</dropdownData>
					</form_elements>
					<updateQuery>
					update professional_profile set is_studying_further_after_degree=':is_studying_further_after_degree',interested_in_type_of_course=':interested_in_type_of_course',area_of_interest=':area_of_interest'  where user_id=:USER_ID;
					</updateQuery>
				</steps>
			</value>
		</entry>
