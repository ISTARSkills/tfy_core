						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select distinct parent_value as key, parent_value as value  from parent_dropdown_table where group_name='ug_degrees' and lower(parent_value) like '%:search_term%'
						</itemSource>
						</dropdownData>
