						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select child_dropdown_value as key, child_dropdown_value as value from child_dropdown_table where parent_value=':dependency_term' and lower(child_dropdown_value) like '%:search_term%' ;
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="7">
						<dataType>number</dataType>
						<label>UG Marks</label>
						<elemntName>ug_marks</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<form_elements id="8">
						<dataType>string</dataType>
						<label>PG Degrees</label>
						<elemntName>pg_degree</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select distinct parent_value as key, parent_value as value  from parent_dropdown_table where group_name='pg_degrees' and lower(parent_value) like '%:search_term%'
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="9" dependency="8">
						<dataType>string</dataType>
						<label>PG Specialization</label>
						<elemntName>pg_specialization</elemntName>
						<elemntType>DROP_DOWN</elemntType>
						<dropdownData type="ajaxified_list" dataFetchType="sql_string">
						<itemSource>
						select child_dropdown_value as key, child_dropdown_value as value from child_dropdown_table where parent_value=':dependency_term' and lower(child_dropdown_value) like '%:search_term%' ;
						</itemSource>
						</dropdownData>
					</form_elements>
					<form_elements id="10">
						<dataType>number</dataType>
						<label>PG Marks</label>
						<elemntName>pg_marks</elemntName>
						<elemntType>TEXT_BOX</elemntType>
					</form_elements>
					<updateQuery>
					 update professional_profile set marks_10=:marks_10,marks_12=:marks_12,yop_10=:yop10,yop_12=:yop12,under_graduation_specialization_name=':ug_specialization',under_gradution_marks=:ug_marks,post_graduation_specialization_name=':pg_specialization',post_gradution_marks=:pg_marks,under_graduate_degree_name=':ug_degree',pg_degree_name=':pg_degree' where user_id=:USER_ID;
